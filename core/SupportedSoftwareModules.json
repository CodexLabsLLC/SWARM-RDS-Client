{
    "SupportedModules": {
        "SWARMVersion": "1.0.0",
        "ValidModuleParameters": [
            "Level",
            "States",
            "Algorithm",
            "Publishes",
            "Subscribes",
            "Parameters"
        ],
        "ValidAlgorithmParameters": [
            "Parameters",
            "ClassName",
            "InputArgs",
            "ReturnValues",
            "FunctionName"
        ],
        "ValidModuleNames": [
            "HighLevelBehavior",
            "LowLevelPathPlanning",
            "HighLevelPathPlanning",
            "ObstacleAvoidance",
            "Detector",
            "VideoRecorder",
            "Mapping"
        ],
        "ValidMessageTypes": [
            "MovementCommand",
            "Trajectory",
            "Position",
            "LinearVelocity",
            "AngularVelocity",
            "AgentState",
            "AngularAcceleration",
            "LinearAcceleration",
            "Quaternion",
            "OccupancyMap",
            "Heading",
            "Speed"
        ],
        "Mapping": {
            "ValidClassNames": [
                "SWARMOccupancyMap"
            ],
            "ValidParameters": {
                "SWARMOccupancyMap": {
                    "Type": {
                        "type": "str",
                        "description": "The Type of Map to Display",
                        "options": [
                            "OccupancyMap"
                        ]
                    },
                    "Center": {
                        "type": "dict",
                        "description": "The center position of the map in NED coordinates relative to the agent",
                        "valid_fields": [
                            "X",
                            "Y",
                            "Z"
                        ],
                        "field_data_type": "float",
                        "field_range": [
                            -99.0,
                            99.0
                        ]
                    },
                    "Size": {
                        "type": "dict",
                        "description": "The Size of the map from the origin to one edge of the map in meters",
                        "valid_fields": [
                            "X",
                            "Y",
                            "Z"
                        ],
                        "field_data_type": "int",
                        "field_range": [
                            0.0,
                            500.0
                        ]
                    },
                    "UpdateRate": {
                        "type": "float",
                        "description": "How often the map should update in seconds. Set to 0.0 if you only want to run it once",
                        "range": [
                            0.0,
                            5.0
                        ]
                    },
                    "ResolutionInMeters": {
                        "type": "float",
                        "description": "The resolution of the map (ie. the size of 1 grid square in the map) in meters",
                        "range": [
                            0.1,
                            5.0
                        ]
                    }
                }
            },
            "ValidInputArgs": {
                "SWARMOccupancyMap": []
            },
            "ValidReturnValues": {
                "SWARMOccupancyMap": [
                    "OccupancyMap"
                ]
            }
        },
        "HighLevelBehavior": {
            "ValidClassNames": [
                "PathFollowing"
            ],
            "ValidParameters": {
                "PathFollowing": {
                    "completed_waypoint_distance": {
                        "type": "float",
                        "description": "The distance to consider a waypoint completed in meters",
                        "range": [
                            0.1,
                            3.0
                        ]
                    }
                }
            },
            "ValidInputArgs": {
                "PathFollowing": [
                    "Trajectory"
                ]
            },
            "ValidReturnValues": {
                "PathFollowing": [
                    "Position",
                    "Heading",
                    "Speed"
                ]
            }
        },
        "LowLevelPathPlanning": {
            "ValidClassNames": [
                "PassThroughPlanner",
                "LinePlanner"
            ],
            "ValidParameters": {
                "PassThroughPlanner": {},
                "LinePlanner": {}
            },
            "ValidInputArgs": {
                "PassThroughPlanner": [
                    "MovementCommand"
                ],
                "LinePlanner": [
                    "MovementCommand"
                ]
            },
            "ValidReturnValues": {
                "PassThroughPlanner": [
                    "MovementCommand"
                ],
                "LinePlanner": [
                    "MovementCommand"
                ]
            },
            "ValidModuleParameters":{
                "completed_waypoint_distance": {
                    "type": "float",
                    "description": "The distance in meters to deem a waypoint 'completed'",
                    "range": [
                        0.1,
                        5.0
                    ]
                },
                "TakeoffAltitude": {
                    "type": "float",
                    "description": "The distance above the ground in meters to takeoff",
                    "range": [
                        0.1,
                        5.0
                    ]
                }
            }
        },
        "HighLevelPathPlanning": {
            "ValidClassNames": [
                "AStar2D"
            ],
            "ValidParameters": {
                "AStar2D": {
                    "resolution": {
                        "type": "float",
                        "description": "The Resolution of the Voxel Grid map in meters",
                        "range": [
                            0.1,
                            5.0
                        ]
                    },
                    "goal_point": {
                        "type": "list",
                        "description": "The X and Y coordinates that are the goal in NED in meters",
                        "length": 2,
                        "field_data_type": "float",
                        "field_range": [
                            -999.0,
                            999.0
                        ]
                    },
                    "map_size": {
                        "type": "list",
                        "description": "The Resolution of the Voxel Grid map in meters",
                        "length": 2,
                        "field_data_type": "float",
                        "field_range": [
                            -999.0,
                            999.0
                        ]
                    },
                    "agent_radius": {
                        "type": "float",
                        "description": "The radius of the agent in meters",
                        "range": [
                            0.5,
                            2.0
                        ]
                    },
                    "flight_altitude": {
                        "type": "float",
                        "description": "The altitude to fly the agent at in meters in NED (so negative is up)",
                        "range": [
                            -0.5,
                            -99.0
                        ]
                    }
                }
            },
            "ValidInputArgs": {
                "AStar2D": [
                    "OccupancyMap"
                ]
            },
            "ValidReturnValues": {
                "AStar2D": [
                    "Trajectory"
                ]
            }
        },
        "VideoRecorder": {
            "ValidClassNames": [
                "VideoRecorder"
            ],
            "ValidParameters": {
                "VideoRecorder": {}
            },
            "ValidInputArgs": {
                "VideoRecorder": []
            },
            "ValidReturnValues": {
                "VideoRecorder": []
            }
        },
        "Detector": {
            "ValidClassNames": [
                "SWARMDetector"
            ],
            "ValidParameters": {
                "SWARMDetector": {}
            },
            "ValidInputArgs": {
                "SWARMDetector": {}
            },
            "ValidReturnValues": {
                "SWARMDetector": []
            }
        },
        "ObstacleAvoidance": {
            "ValidClassNames": [
                "SlopeDetection"
            ],
            "ValidParameters": {
                "SlopeDetection": {
                    "min_slope": {
                        "type": "float",
                        "description": "The minimum slope of the terrain in degrees to acivate the algorithm.",
                        "range": [
                            5.0,
                            40.0
                        ]
                    },
                    "max_slope": {
                        "type": "float",
                        "description": "The maximum slope of the terrain in degrees to acivate the algorithm.",
                        "range": [
                            50.0,
                            85.0
                        ]
                    }
                }
            },
            "ValidInputArgs": {
                "SlopeDetection": []
            },
            "ValidReturnValues": {
                "SlopeDetection": [
                    "Position",
                    "LinearVelocity",
                    "Heading"
                ]
            }
        }
    }
}